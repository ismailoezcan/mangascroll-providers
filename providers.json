            "title": ".font-black",
            "url": ".relative",
            "poster": "img"
        },
        "details": {
            "title": "h1",
            "poster": "img",
            "description": ".text-sm.text--secondary",
            "authors": "",
            "genres": "div > a.text-brand.text-sm.mr-1",
            "status": "div.grid.grid-cols-1.mb-3 > div:nth-child(2) > div",
            "seriesType": ".container .grid > div:nth-child(1) > div"
        },
        "chapters": {
            "container": "#chapters > div > a",
            "title": "",
            "number": "",
            "date": "",
            "url": ""
        },
        "pages": {
            "container": "img.js-page",
            "imageAttribute": "data-src"
        }
    },
    "parsing": {
        "dateFormat": "noOp",
        "chapterRegex": "(?:Group (\\d+))? ?Chapter (\\d+(\\.\\d+)?)",
        "statusMapping": {
            "publishing": "ONGOING",
            "finished": "COMPLETED",
            "on hiatus": "HIATUS",
            "discontinued": "DROPPED",
            "_default": "ONGOING"
        }
    },
    "imageConfig": {
        "referrer": "https://mangapill.com"
