{
  "name": "CulturedWorks",
  "baseUrl": "https://culturedworks.com",
  "selectors": {
    "pages": {
      "container": ".readerarea img",
      "imageAttribute": "data-src"
    },
    "search": {
      "url": "a",
      "title": ".tt",
      "poster": "img",
      "container": ".listupd > .bs",
      "searchUrl": "/manga"
    },
    "details": {
      "title": ".entry-title",
      "genres": ".meta .genres.mt-10 .genre-item",
      "poster": ".img-thumb img.wp-post-image",
      "status": ".rating-status.mt-5 .status",
      "authors": "div.tsinfo.bixbox > div:nth-child(3) > i",
      "seriesType": "div.tsinfo.bixbox > div:nth-child(1) > a",
      "description": ".entry-content p"
    },
    "chapters": {
      "url": "a",
      "date": ".chapterdate",
      "title": ".chapternum",
      "container": "#chapterlist li"
    }
  },
  "parsing": {
    "dateFormat": "standard",
    "chapterRegex": "Chapter (\\d+(\\.\\d+)?)",
    "statusMapping": {
      "hiatus": "HIATUS",
      "dropped": "DROPPED",
      "ongoing": "ONGOING",
      "completed": "COMPLETED"
    }
  },
  "imageConfig": null
}