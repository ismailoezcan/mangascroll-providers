{
  "name": "VoidScans",
  "baseUrl": "https://hivetoon.com",
  "selectors": {
    "pages": {
      "container": ".ts-main-image",
      "imageAttribute": "src"
    },
    "search": {
      "url": "a",
      "title": ".tt",
      "poster": "img",
      "container": ".listupd > .bs > .bsx",
      "searchUrl": "/projects/page/{pageNumber}/"
    },
    "details": {
      "title": "h1.entry-title",
      "genres": ".mgen a",
      "poster": ".info-left-margin .wp-post-image",
      "status": ".info-left .imptdt:nth-child(1) i",
      "authors": ".info-left .imptdt:nth-child(3) i",
      "seriesType": ".info-left .imptdt a",
      "description": ".entry-content p"
    },
    "chapters": {
      "url": "a",
      "date": ".chapterdate",
      "title": ".chapternum",
      "container": "#chapterlist li"
    }
  },
  "parsing": {
    "dateFormat": "standard",
    "volumeRegex": "Volume (\\d+(\\.\\d+)?)",
    "chapterRegex": "Chapter (\\d+(\\.\\d+)?)",
    "statusMapping": {
      "hiatus": "HIATUS",
      "dropped": "DROPPED",
      "ongoing": "ONGOING",
      "completed": "COMPLETED"
    }
  },
  "imageConfig": null
}